#! /bin/sh

# linux_atory.run - Script de ejecución para Linux
#
# $Revision$

# Directorio que tomaremos como compartido
SHARE_DIR="$HOME/Atory_Share"
# Fichero de configuración
RCFILE="$HOME/.atoryrc"

cd `dirname $0` 
LD_LIBRARY_PATH='.' java -cp atory.jar:swt.jar:xpp3-1.3.3_7.jar -Dsharedir="$SHARE_DIR" -Drcfile="$RCFILE" atory.Atory

